{
  "TokenOptions": {
    "Audience": "serkan@serkan.com", //JWT içinde olması gereken alanlardan biri.Projenin web adresi vs. verilebilir.Türkçesi kitle demektir.
    "Issuer": "serkan@serkan.com", //Uygulayan demek.WT içinde olması gereken alanlardan biri.
    "AccessTokenExpiration": 10, //Verdiğimiz token'ın geçerlilik süresi belirtilir.Dakika cinsindendir.Biz burada 10 dakika dedik. JWT içinde olması gereken alanlardan biri.Burası çok önemli.
    "SecurityKey": "mysupersecretkeymysupersecretkey" //Token'ı oluştururken(Asp .Net'tin)kullanacağı anahtardır.JWT içinde olması gereken alanlardan biri.Burası çok önemli.
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
